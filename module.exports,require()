var w = divisao(2,3);
console.log(w);

//soma.js
var soma = function(a,b){
    return a+b;
}

module.exports = soma;

//subtracao.js
var subtracao = function(a,b){
    return a-b;
}

module.exports = subtracao;

//divisao.js
var divisao = function(a,b){
    return a/b;
}

module.exports = divisao;

//multiplicacao.js
var multiplicacao = function(a,b){
    return a*b;
}

module.exports = multiplicacao;

